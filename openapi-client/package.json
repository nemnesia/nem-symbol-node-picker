{
  "name": "openapi-client",
  "version": "1.0.0",
  "volta": {
    "node": "22.21.0",
    "npm": "11.6.2",
    "yarn": "4.10.3"
  },
  "scripts": {
    "gen:ts": "openapi-generator-cli generate -i openapi.yaml -g typescript-fetch -o ./dist/typescript/openapi-client",
    "gen:py": "openapi-generator-cli generate -i openapi.yaml -g python -o ./dist/python/openapi-client",
    "gen:php": "openapi-generator-cli generate -i openapi.yaml -g php -o ./dist/php/openapi-client",
    "gen:dart": "openapi-generator-cli generate -i openapi.yaml -g dart -o ./dist/dart/openapi-client",
    "get:openapi": "curl -so openapi.yaml https://nodewatch.symbol.tools/static/openapi/openapi.yaml"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.25.0"
  }
}
