{
  "name": "nem-symbol-node-picker",
  "version": "1.0.0",
  "description": "A lightweight Node.js package for picking random NEM and Symbol blockchain nodes from NodeWatch API",
  "keywords": [
    "nem",
    "symbol",
    "blockchain",
    "node",
    "picker",
    "nodejs"
  ],
  "author": "ccHarvestasya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemnesia/nem-symbol-node-picker.git"
  },
  "bugs": {
    "url": "https://github.com/nemnesia/nem-symbol-node-picker/issues"
  },
  "homepage": "https://github.com/nemnesia/nem-symbol-node-picker#readme",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "22.21.1",
    "yarn": "4.11.0"
  },
  "type": "module",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:watch": "rollup -c --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint \"{src,test,e2e}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\"",
    "test": "vitest run test",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run e2e",
    "test:e2e:watch": "vitest --watch e2e",
    "test:all": "vitest run && vitest run e2e",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish",
    "install:local-module": "yarn add @nemnesia/nodewatch-openapi-fetch-client@portal:../../nodewatch-openapi-fetch-client/typescript"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rollup": "^4.53.2",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@nemnesia/nodewatch-openapi-fetch-client": "portal:../../nodewatch-openapi-fetch-client/typescript"
  }
}
